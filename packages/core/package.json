{
  "name": "cjs-shim",
  "version": "1.0.0",
  "type": "module",
  "description": "CJS/Node shims",
  "dependencies": {
    "magic-string": "^0.30.21",
    "typescript": "^5.9.3"
  },
  "files": [
    "src",
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./shim": {
      "types": "./build/shim.d.ts",
      "import": "./build/shim.js",
      "require": "./build/shim.cjs"
    },
    "./*": "./*"
  },
  "keywords": [
    "shim",
    "cjs",
    "node",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/cjs-shim.git"
  },
  "author": "startracex",
  "license": "MIT"
}
