{
  "name": "rollup-plugin-cjs-shim",
  "version": "2.0.0",
  "type": "module",
  "description": "CJS/Node shim plugin",
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "cjs-shim": "workspace:^"
  },
  "files": [
    "src",
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/cjs-shim.git"
  },
  "author": "startracex",
  "license": "MIT"
}
