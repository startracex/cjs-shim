{
  "name": "unplugin-cjs-shim",
  "version": "1.0.0",
  "type": "module",
  "description": "CJS/Node shim plugins",
  "dependencies": {
    "cjs-shim": "workspace:^",
    "unplugin": "^2.3.10"
  },
  "devDependencies": {
    "@farmfe/core": "^1.7.11",
    "@nuxt/kit": "^4.2.0",
    "@nuxt/schema": "^4.2.0",
    "esbuild": "^0.25.11",
    "rolldown": "1.0.0-beta.44",
    "rollup": "^4.52.5",
    "vite": "^7.1.12",
    "webpack": "^5.102.1"
  },
  "peerDependencies": {
    "@nuxt/kit": "*"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./build/*.js",
      "require": "./build/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "shim",
    "cjs",
    "node",
    "unplugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/cjs-shim.git"
  },
  "author": "startracex",
  "license": "MIT"
}
